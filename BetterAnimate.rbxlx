local WaitBeforeUpdatePose = 0.02 -- Bigger value = less script activity + more unstable
local WaitBeforeUpdateAnimationSpeed = 0.15
local WaitBeforeFallAnimationPlayAfterJump = 0.31 -- Wait if character jumped
