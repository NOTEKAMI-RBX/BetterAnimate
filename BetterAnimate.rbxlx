			--[[
				local Range = NumberRange.new(0, 9)
				
				Idle: Speed > -math.huge and Speed <= Range.Min (0)
				Walk: Speed > Range.Min (0) and Speed <= Range.Max (9)
				Run: Speed > Range.Max (9) and Speed <= math.huge
